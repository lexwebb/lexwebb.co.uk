    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.4.0",
    "react-markdown": "^8.0.3",
    "react-p5": "^1.3.30",
    "sass": "^1.55.0",
    "use-dark-mode": "^2.3.1",
    "usehooks-ts": "^2.7.2"
  },
  "devDependencies": {
    "@types/node": "18.8.0",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "eslint": "8.24.0",
    "eslint-config-next": "12.3.1",
